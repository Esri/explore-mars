{
  "name": "explore-mars",
  "version": "1.0.0",
  "description": "Interactively discover the red planet using the ArcGIS Maps SDK for JavaScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/Esri/explore-mars"
  },
  "type": "module",
  "author": "Gunnar Ingi Stefansson",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "concurrently --group --prefix none \"npm:lint:*\"",
    "lint:typecheck": "tsc --noEmit",
    "lint:eslint": "npx eslint",
    "lint:prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,scss}'",
    "lint:stylelint": "stylelint --cache --fix 'src/**/*.{css,scss}'"
  },
  "dependencies": {
    "@arcgis/core": "^4.29.10",
    "@esri/calcite-components": "^2.7.1",
    "@zip.js/zip.js": "^2.7.29",
    "classnames": "^2.5.1",
    "gl-matrix": "^3.4.3",
    "ts-pattern": "^5.1.0",
    "typescript": "^5.4.3",
    "vite": "^5.2.8"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "prettier": "^3.2.5",
    "sass": "^1.74.1",
    "stylelint": "^16.3.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript-eslint": "^7.5.0"
  }
}
